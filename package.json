{
  "name": "node-jq",
  "version": "0.0.0-automated-versioning",
  "description": "Run jq in node",
  "main": "lib/jq",
  "repository": {
    "type": "git",
    "url": "https://github.com/sanack/node-jq"
  },
  "scripts": {
    "pretest": "npm run install-binary",
    "install-binary": "node scripts/install-binary.js",
    "test": "mocha",
    "test:watch": "npm test -- --watch",
    "lint": "standard *.ts --verbose | snazzy",
    "build": "tsc -p ./",
    "postinstall": "npm run install-binary",
    "coverage": "nyc mocha",
    "precodeclimate": "npm run coverage",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "engines": {
    "npm": ">=6.0.0"
  },
  "keywords": [
    "jq",
    "json"
  ],
  "author": "sanack",
  "contributors": [
    {
      "name": "davesnx",
      "email": "dsnxmoreno@gmail.com"
    },
    {
      "name": "mackermans",
      "email": "maarten.ackermans@gmail.com"
    }
  ],
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "scripts",
    "docs"
  ],
  "dependencies": {
    "@types/chai": "^4.2.0",
    "bin-build": "^3.0.0",
    "download": "^7.1.0",
    "is-valid-path": "^0.1.1",
    "strip-eof": "^1.0.0",
    "strip-final-newline": "^2.0.0",
    "tempfile": "^3.0.0"
  },
  "devDependencies": {
    "@hapi/joi": "16.0.0-preview",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "3.0.6",
    "dts-gen": "^0.5.8",
    "eslint": "5.0.0",
    "husky": "3.0.3",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.2",
    "snazzy": "^8.0.0",
    "source-map-support": "^0.5.13",
    "standard": "^13.0.1",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.3.0",
    "tslint": "5.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3",
    "validate-commit-msg": "^2.14.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "global": [
      "describe",
      "it"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test",
      "commit-msg": "validate-commit-msg"
    }
  }
}
